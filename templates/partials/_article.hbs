{
    "authorV2": {
        "name": "{{faker 'person.fullName'}}",
        "firstName": "{{faker 'person.firstName'}}",
        "lastName": "{{faker 'person.lastName'}}",
        "url": "{{faker 'internet.url'}}",
        "email": "{{faker 'internet.email'}}",
        "bio": {{{nlbr (faker 'lorem.lines' min=3 max=5)}}}
    },
    "content": {{{nlbr (faker 'lorem.lines' min=3 max=5)}}},
    "contentHtml": {{{nlbr (faker 'lorem.lines' min=20 max=50)}}},
    "excerpt": {{{nlbr (faker 'lorem.lines' min=1 max=2)}}},
    "excerptHtml": {{{nlbr (faker 'lorem.lines' min=1 max=2)}}},
    "id": "{{faker 'string.uuid'}}",
    "handle": "{{faker 'string.uuid'}}",
    "image": {{> image}},
    "onlineStoreUrl": "{{faker 'internet.url'}}",
    "publishedAt": "{{faker 'date.past'}}",
    "tags": [
        {{#repeat 3}}
            "{{faker 'lorem.word'}}"
        {{/repeat}}
    ],
    "title": "A Mock Shopify article",
    "trackingPartners": "{{faker 'lorem.word'}}",
    "seo": {
        "title": "A mock Shopify article",
        "description": "A mock Shopify article using a mock shopify server"
    },
    "metatfield": {
        "key": "{{faker 'lorem.word'}}",
        "namespace": "{{faker 'lorem.word'}}"
    },
    "metafields": [
        {{#repeat 3}}
            {
                "key": "{{faker 'lorem.word'}}",
                "namespace": "{{faker 'lorem.word'}}"
            }
        {{/repeat}}
    ],
    "comments": [
        {{#repeat 3}}
            {
                "author": "{{faker 'person.fullName'}}",
                "body": "{{faker 'lorem.sentence'}}",
                "bodyHtml": "{{faker 'lorem.sentence'}}",
                "id": "{{faker 'string.uuid'}}",
                "publishedAt": "{{faker 'date.past'}}",
                "url": "{{faker 'internet.url'}}"
            }
        {{/repeat}}
    ],
    "blog": {{> blog}}
}