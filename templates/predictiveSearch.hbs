{
    "queries": [
        {
             "styledText": "{{faker "lorem.word"}} {{context.tree.predictiveSearch.__args.query}}",
            "text": "{{context.tree.predictiveSearch.__args.query}} {{faker 'lorem.word'}}"
        },
        {
            "styledText": "{{faker "lorem.word"}} {{context.tree.predictiveSearch.__args.query}}",
            "text": "{{faker 'lorem.word'}} {{context.tree.predictiveSearch.__args.query}}"
        }
    ],
    "products": [
        {{#unless (eq context.tree.predictiveSearch.__args.query "no-results")}}
            {{#repeat (select context.tree.predictiveSearch.__args.first 10)}} 
                {{> product }}
            {{/repeat}}
        {{/unless}}
    ],
    "collections": [
         {{#unless (eq context.tree.predictiveSearch.__args.query "no-results")}}
            {{#repeat (select context.tree.predictiveSearch.__args.first 10)}} 
                {{> collection }}
            {{/repeat}}
        {{/unless}}
    ],
    "articles": [
        {{#unless (eq context.tree.predictiveSearch.__args.query "no-results")}}
            {{#repeat (select context.tree.predictiveSearch.__args.first 10)}} 
                {{> article }}
            {{/repeat}}
        {{/unless}}
    ],
    "pages": [
        {{#unless (eq context.tree.predictiveSearch.__args.query "no-results")}}
            {{#repeat (select context.tree.predictiveSearch.__args.first 10)}} 
                {{> page }}
            {{/repeat}}
        {{/unless}}
    ]
}